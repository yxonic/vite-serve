{
  "name": "vite-serve",
  "version": "0.4.4",
  "description": "instantly preview a single file component with vite",
  "bin": "dist/main.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,vue}\"",
    "lint": "eslint --fix --ext .ts,.tsx,.js,.jsx,.vue src",
    "build": "tsc",
    "tc": "tsc --noEmit",
    "prepare": "husky install",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^1.3.1",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@yxonic/vmark": "^0.2.10",
    "cac": "^6.7.12",
    "picocolors": "^1.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "shiki": "^0.10.1",
    "styled-components": "^5.3.5",
    "vite": "^2.9.5",
    "vite-plugin-windicss": "^1.8.4",
    "vue": "^3.2.33",
    "windicss": "^3.5.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.0",
    "prettier": "^2.6.2",
    "semantic-release": "^19.0.2",
    "typescript": "^4.6.3"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx,vue}": [
      "eslint --fix --max-warnings=0"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yxonic/vite-serve.git"
  },
  "author": "Yu Yin <yxonic@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/yxonic/vite-serve/issues"
  },
  "homepage": "https://github.com/yxonic/vite-serve#readme",
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  }
}
